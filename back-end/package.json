{
  "name": "games-backend",
  "version": "2.5.0",
  "description": "Back end for a website to play some simple games",
  "main": "server.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint . --ext ts,js --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --config .prettierrc --end-of-line lf --write .",
    "prod:deploy": "prisma migrate deploy && node server.js",
    "studio": "prisma studio"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.96",
    "@swc/helpers": "^0.5.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^5.5.2",
    "regenerator-runtime": "^0.14.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
